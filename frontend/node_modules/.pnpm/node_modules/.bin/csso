#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/csso@2.3.2/node_modules/csso/bin/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/csso@2.3.2/node_modules/csso/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/csso@2.3.2/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/csso@2.3.2/node_modules/csso/bin/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/csso@2.3.2/node_modules/csso/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/csso@2.3.2/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../csso/bin/csso" "$@"
else
  exec node  "$basedir/../csso/bin/csso" "$@"
fi
