#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/js-yaml@3.7.0/node_modules/js-yaml/bin/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/js-yaml@3.7.0/node_modules/js-yaml/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/js-yaml@3.7.0/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/js-yaml@3.7.0/node_modules/js-yaml/bin/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/js-yaml@3.7.0/node_modules/js-yaml/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/js-yaml@3.7.0/node_modules:/Users/rodrigocauduro/Documents/01Business/language-game/language-games/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
else
  exec node  "$basedir/../js-yaml/bin/js-yaml.js" "$@"
fi
